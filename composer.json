{
    "name": "lukasbableck/contao-tobii-lightbox-bundle",
    "type": "contao-bundle",
	"description": "This bundle adds the accessible, open-source Tobii lightbox to Contao.",
	"keywords": [
		"lightbox",
		"image",
		"accessibility",
		"accessible",
		"a11y"
	],
    "license": "LGPL-3.0-or-later",
	"authors": [
		{
			"name": "Lukas Bableck",
			"email": "lukas@bableck.dev",
			"homepage": "https://bableck.dev"
		}
	],
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^5.3"
    },
	"conflict": {
		"contao/manager-plugin": "<2.0 || >=3.0"
	},
	"require-dev": {
		"contao/manager-plugin": "^2.0",
		"friendsofphp/php-cs-fixer": "^3.57"
	},
    "autoload": {
        "psr-4": {
            "Lukasbableck\\ContaoTobiiLightboxBundle\\": "src/"
        }
    },
	"extra": {
		"contao-manager-plugin": "Lukasbableck\\ContaoTobiiLightboxBundle\\ContaoManager\\Plugin"
	}
}
